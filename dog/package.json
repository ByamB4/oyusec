{
  "name": "oyusec-dog",
  "version": "5.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:staging": "env-cmd -f .env.staging yarn build && yarn start",
    "export": "NODE_ENV=production next build && next export",
    "start": "next start",
    "lint": "yarn lint:tsc && yarn lint:eslint && yarn lint:prettier",
    "lint:tsc": "tsc --noEmit",
    "lint:eslint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:prettier": "prettier --check .",
    "fix": "yarn fix:eslint && yarn fix:prettier",
    "fix:eslint": "eslint . --fix --ext .js,.jsx,.ts,.tsx",
    "fix:prettier": "prettier --write .",
    "lint:conflict": "eslint-config-prettier .eslintrc.json",
    "postinstall": "typesync",
    "check": "typesync && npm outdated && npm audit && yarn lint:conflict && yarn lint && yarn dev"
  },
  "lint-staged": {
    "*.{js,ts,tsx, jsx}": [
      "eslint --fix"
    ],
    "*.{js,jsx,ts,tsx}": [
      "prettier --write"
    ]
  },
